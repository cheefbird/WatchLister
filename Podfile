# Uncomment the next line to define a global platform for your project
platform :ios, '11.0'

target 'WatchLister' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Alamofire'
  pod 'SwiftyJSON'
  pod 'RxSwift', :git => 'https://github.com/ReactiveX/RxSwift.git', :branch => 'rxswift4.0-swift4.0', :tag => '4.0.0-rc.0'
  pod 'RxCocoa', :git => 'https://github.com/ReactiveX/RxSwift.git', :branch => 'rxswift4.0-swift4.0', :tag => '4.0.0-rc.0'
  pod 'RealmSwift'
  

  target 'WatchListerTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
